{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022", "DOM"],
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "types": ["cypress", "node"],
    "skipLibCheck": true,
    "noEmit": true,
    "strict": false,
    "baseUrl": "."
  },
  "include": [
    "e2e/**/*.ts",
    "e2e/**/*.tsx",
    "support/**/*.ts",
    "support/**/*.tsx",
    "harness/**/*.ts",
    "harness/**/*.tsx",
    "plugins/**/*.ts",
    "plugins/**/*.tsx"
  ]
}
